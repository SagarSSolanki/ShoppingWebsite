<%= form_with model: @address do |form| %>
  <p>
    <%= form.label :area %><br>
    <%= form.text_field :area %>
    <% @address.errors.full_messages_for(:area).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </p>
  <p>
    <%= form.label :city %><br>
    <%= form.text_field :city %>
    <% @address.errors.full_messages_for(:area).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </p>
  <p>
    <%= form.label :state %><br>
    <%= form.text_field :state %>
    <% @address.errors.full_messages_for(:area).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </p>
  <p>
    <%= form.label :pincode %><br>
    <%= form.number_field :pincode %>
    <% @address.errors.full_messages_for(:area).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
  
